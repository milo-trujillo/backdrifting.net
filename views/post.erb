<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "NewsArticle",
  "mainEntityOfPage": {
	"@type": "WebPage",
	"@id": "https://google.com/article"
  },
  "headline": "<%= pagetitle %>",
  "datePublished": "<%= post_date %>",
